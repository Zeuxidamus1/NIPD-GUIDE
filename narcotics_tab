    {imprint:"T 173", name:"Methylphenidate (Concerta/Ritalin)", schedule:"II", statute:"R.S. 40:967"},
    {imprint:"M 1810 10 mg", name:"Methylphenidate", schedule:"II", statute:"R.S. 40:967"},
    {imprint:"215", name:"Methylphenidate", schedule:"II", statute:"R.S. 40:967"},
    {imprint:"K 101", name:"Methylphenidate", schedule:"II", statute:"R.S. 40:967"},
    {imprint:"M 10", name:"Methylphenidate", schedule:"II", statute:"R.S. 40:967"},
    {imprint:"M 20", name:"Methylphenidate", schedule:"II", statute:"R.S. 40:967"}
];
  const norm = s => (s||"").toUpperCase().replace(/[^A-Z0-9]/g,"");
  function filterImprints(prefix){
    const n = norm(prefix);
    if(n.length<2) return [];
    return QUICK_IMPRINTS.filter(x => norm(x.imprint).startsWith(n)).slice(0,50);
  }

  // unified query (replaces separate imprint/name boxes)
  const [query,setQuery]=React.useState("");
  const [selected,setSelected]=React.useState([]);

  function addSelected(item){ setSelected(list=>[...list, item]); }
  function removeSelected(idx){ setSelected(list=>list.filter((_,i)=>i!==idx)); }

  function openPillIdentifier(){
    if(!query.trim()) return;
    const url = "https://www.drugs.com/imprints.php?imprint=" + encodeURIComponent(query.trim());
    window.open(url, "_blank", "noopener");
  }

  // Name hits from QUICK_PILL_DB (prefix/contains match)
  function nameMatches(q){
    const s = (q||"").trim().toLowerCase();
    if(s.length<2) return [];
    const results = [];
    for(const [key,val] of Object.entries(QUICK_PILL_DB)){
      if(key.includes(s) || (val.aka||[]).some(a=>a.toLowerCase().includes(s))){
        results.push({imprint:key.toUpperCase(), name:key, schedule:val.schedule, statute:val.statute, source:"name"});
      }
    }
    return results.slice(0,50);
  }

  return React.createElement("div",{className:"section"},
    React.createElement(Box,{title:""},
      React.createElement(Row,{label:"Imprint or Name"},
        React.createElement("input",{className:"input",placeholder:"e.g., M367, RP 10 325, Xanax, Suboxone",value:query,onChange:e=>setQuery(e.target.value)})
      ),
      (function(){
        const imHits = filterImprints(query);
        const nmHits = nameMatches(query);
        const hasHits = imHits.length>0 || nmHits.length>0;
        if(!hasHits) return null;
        return React.createElement("div",{className:"card", style:{marginTop:"8px"}},
          React.createElement("div",{className:"small muted"},"Matches:"),
          imHits.length>0 && React.createElement("div",null,
            React.createElement("div",{className:"xsmall muted",style:{margin:"6px 0 2px"}}, "Imprints"),
            React.createElement("div",{style:{maxHeight:"140px",overflowY:"auto", borderBottom:"1px solid #eee", paddingBottom:"6px"}},
              imHits.map((x,i)=>(function(){
                const sc=(x.schedule||"").toString().toUpperCase().replace(/[^A-Z0-9]/g,"");
                return React.createElement("div",{key:"i"+i,style:{display:"flex",justifyContent:"space-between",alignItems:"center",padding:"6px 0"}},
                  React.createElement("div",null,
                    React.createElement("div",{className:"small"},
                      React.createElement("strong",null,x.imprint),
                      React.createElement("span",{className:"pill-badge schedule-"+(sc||"NA")}, x.schedule||"—")
                    ),
                    React.createElement("div",{className:"xsmall muted"}, x.name)
                  ),
                  React.createElement("div",{style:{display:"flex",gap:"6px"}},
                    React.createElement("button",{className:"btn",onClick:()=>{addSelected(x);}}, "Select")
                  )
                );
              })())
            )
          ),
          nmHits.length>0 && React.createElement("div",null,
            React.createElement("div",{className:"xsmall muted",style:{margin:"6px 0 2px"}}, "Names"),
            React.createElement("div",{style:{maxHeight:"140px",overflowY:"auto"}},
              nmHits.map((x,i)=>(function(){
                const sc=(x.schedule||"").toString().toUpperCase().replace(/[^A-Z0-9]/g,"");
                return React.createElement("div",{key:"n"+i,style:{display:"flex",justifyContent:"space-between",alignItems:"center",padding:"6px 0"}},
                  React.createElement("div",null,
                    React.createElement("div",{className:"small"},
                      React.createElement("strong",null,x.name.toUpperCase()),
                      React.createElement("span",{className:"pill-badge schedule-"+(sc||"NA")}, x.schedule||"—")
                    ),
                    React.createElement("div",{className:"xsmall muted"}, (x.imprint||"").toString())
                  ),
                  React.createElement("div",{style:{display:"flex",gap:"6px"}},
                    React.createElement("button",{className:"btn",onClick:()=>{addSelected(x);}}, "Select")
                  )
                );
              })())
            )
          ),
          React.createElement("div",{className:"xsmall muted",style:{marginTop:"6px"}},
            "Legend: ",
            React.createElement("span",{className:"pill-badge schedule-II"},"II"), " = Schedule II, ",
            React.createElement("span",{className:"pill-badge schedule-III"},"III"), " = Schedule III, ",
            React.createElement("span",{className:"pill-badge schedule-IV"},"IV"), " = Schedule IV, ",
            React.createElement("span",{className:"pill-badge schedule-V"},"V"), " = Schedule V, ",
            React.createElement("span",{className:"pill-badge schedule-NOTCONTROLLED"},"NC"), " = Not controlled"
          )
        );
      })(),
      React.createElement("div",{className:"navrow"},  React.createElement("button",{className:"btn",style:{flex:1},onClick:openPillIdentifier,disabled:!query.trim()},"Search PillIdentifier.com"),  React.createElement("button",{className:"btn danger",style:{flex:1},    onClick:()=>{ if(confirm("Are you sure you want to clear all selected pills and the search field?")){ setSelected([]); setQuery(""); } },    disabled:(selected.length===0 && !query.trim())  },"Clear All")),
      (function(){
        const hit = findScheduleByName(query);
        if(!hit) return React.createElement("div",{className:"small muted", style:{marginTop:"8px"}},"");
        const s = statuteLink(hit.statute||"");
        return React.createElement("div",{className:"card", style:{marginTop:"8px"}},
          React.createElement("div",{className:"small"}, React.createElement("strong",null, (query||"").toUpperCase()), " — Schedule ", hit.schedule),
          React.createElement("div",{className:"small"}, React.createElement("strong",null,"Statute: "), (hit.statute && hit.statute!=="—" ? React.createElement("a",{className:"lawlink",href:s.url,target:"_blank",rel:"noopener"}, s.label) : "—")),
          React.createElement("div",{className:"small muted"},"Verify with the external Pill Identifier page.")
        );
      })()
    ),
    // Bottom section — cumulative selected pills
    (function(){
      if(selected.length===0) return null;
      return React.createElement(Box,{title:"", subtitle:"All items you selected"},
        React.createElement("div",null,
          selected.map((x,idx)=>{
            const sc=(x.schedule||"").toString().toUpperCase().replace(/[^A-Z0-9]/g,"");
            const s = statuteLink(x.statute||"");
            return React.createElement("div",{key:idx,className:"card",style:{
            marginBottom:"6px",
            border:"2px solid #1f4db8",
            background:"#e8efff",
            borderRadius:"14px",
            boxShadow:"0 4px 10px rgba(31,77,184,0.15)",
            padding:"10px 14px"
            
}},


              React.createElement("div",{className:"small"},
                React.createElement("strong",null, x.imprint || x.name.toUpperCase()), x.imprint? " — "+x.name : "",
                React.createElement("span",{className:"pill-badge schedule-"+(sc||"NA"), style:{marginLeft:"8px"}}, x.schedule||"—")
              ),
              React.createElement("div",{className:"xsmall"}, "Statute: ", (x.statute && x.statute!=="—" ? React.createElement("a",{className:"lawlink",href:s.url,target:"_blank",rel:"noopener"}, s.label) : "—")),
              React.createElement("div",{className:"navrow"},
                React.createElement("button",{className:"btn ghost",onClick:()=>removeSelected(idx)}, "Remove")
              )
            );
          })
        )
      );
    })()
  );
}

    /* ---------- Error Boundary ---------- */
    class ErrorBoundary extends React.Component {
      constructor(props){ super(props); this.state={hasError:false,error:null}; }
      static getDerivedStateFromError(error){ return {hasError:true,error}; }
      componentDidCatch(error, info){ console.error("UI error:", error, info); }
      render(){
        if(this.state.hasError){
          return h(
            "div",
            {className:"card", style:{border:"1px solid #fca5a5",background:"#fff1f2",color:"#991b1b",borderRadius:"12px",padding:"12px"}},
            h("div", {className:"small", style:{fontWeight:700}}, "Something went wrong rendering this step."),
            h("div", {className:"small"}, String(this.state.error||"Unknown error"))
          );
        }
        return this.props.children;
      }
    }

    /* ---------- Narcotics Tab Component ---------- */
    function NarcoticsTab(){
      const [showPill,setShowPill]=React.useState(true);
      return h("div",null,
        h("div",{className:"card",style:{padding:"16px"}},
          h("div",{style:{display:"flex",justifyContent:"space-between",alignItems:"center",marginBottom:"8px"}},
            h("h3",{style:{margin:0}},"Pill Identification"),
            h("button",{className:"btn",onClick:()=>setShowPill(v=>!v)}, showPill?"Hide":"Show")
          ),
          showPill ? h(PillLookup) : null
        )
      );
    }

    ReactDOM.createRoot(document.getElementById("root")).render(h(ErrorBoundary,null,h(NarcoticsTab)));
  })();
  </script>

<script>
/* Non-invasive Drug Slang injector — FULL DATA + Compact Schedules + Subheader + Resilient + Reverse + Fuzzy */
(function(){"use strict";
  var __B64 = "";
  var __SB64 = "";
  try{ window.__SLANG_MAP__ = JSON.parse(atob(__B64)); }catch(e){ window.__SLANG_MAP__ = {}; }
  try{ window.__SCHEDULE_MAP__ = JSON.parse(atob(__SB64)); }catch(e){ window.__SCHEDULE_MAP__ = {}; }
  var SLANG_MAP = window.__SLANG_MAP__;
  var SCHEDULE_MAP = window.__SCHEDULE_MAP__;

  var CARD_ID = "drug-slang-card";
  var TOGGLE_KEY = "drug_slang_toggle_open";

  function looksLikePillCard(node){
    if(!node) return false;
    var h3 = node.querySelector && node.querySelector('h3');
    if(!h3) return false;
    var text = (h3.textContent||'').trim().toLowerCase();
    return text.indexOf('pill') !== -1 && (text.indexOf('identification') !== -1 || text.indexOf('lookup') !== -1);
  }

  function createEl(tag, attrs, html){
    var el = document.createElement(tag);
    if(attrs) Object.keys(attrs).forEach(function(k){ el.setAttribute(k, attrs[k]); });
    if(html!=null) el.innerHTML = html;
    return el;
  }

  function ensureModalStyles(){
    if(document.getElementById('slang-modal-style')) return;
    var style = document.createElement('style');
    style.id = 'slang-modal-style';
    style.textContent = '.modal-backdrop{position:fixed;inset:0;background:rgba(0,0,0,.6);display:flex;align-items:center;justify-content:center;z-index:9999}'
      + '.slang-modal{background:#fff;color:#111;border-radius:16px;max-width:760px;width:92vw;max-height:85vh;overflow:auto;box-shadow:0 12px 32px rgba(0,0,0,.35)}'
      + '.slang-modal .hd{padding:14px 16px;border-bottom:1px solid #eee}'
      + '.slang-modal .ttl{font-weight:700}'
      + '.slang-modal .subline{margin-top:4px;color:#444;font-size:.95rem}'
      + '.slang-modal .bd{padding:16px}'
      + '.slang-modal .ft{padding:12px 16px;border-top:1px solid #eee;display:flex;gap:8px;justify-content:flex-end}'
      + '.slang-modal .item{padding:8px 0;border-bottom:1px solid #eee}'
      + '.slang-modal .sub{font-size:.95rem;color:#333}'
      + '.btn{padding:10px 14px;border-radius:12px;border:1px solid #3c4e73;background:#1b2658;color:#fff;cursor:pointer}'
      + '.btn.ghost{background:#f7f9ff;color:#0f1532;border-color:#d0daf5}'
      + '.sched{opacity:.8;font-weight:500}';
    document.head.appendChild(style);
  }

  function buildModal(){
    ensureModalStyles();
    var backdrop = createEl('div',{class:'modal-backdrop'},'');
    var modal = createEl('div',{class:'slang-modal'},'');
    var hd = createEl('div',{class:'hd'},'');
    var title = createEl('div',{class:'ttl'},'Drug Slang —');
    var subline = createEl('div',{class:'subline'},'');
    hd.appendChild(title); hd.appendChild(subline);
    var bd = createEl('div',{class:'bd'},'');
    var ft = createEl('div',{class:'ft'},'');
    var closeBtn = createEl('button',{class:'btn ghost'},'Close');
    closeBtn.addEventListener('click', function(){ document.body.removeChild(backdrop); });
    ft.appendChild(closeBtn);
    modal.appendChild(hd); modal.appendChild(bd); modal.appendChild(ft);
    backdrop.appendChild(modal);
    backdrop.addEventListener('click', function(e){ if(e.target===backdrop) document.body.removeChild(backdrop); });
    return { backdrop:backdrop, title:title, subline:subline, body:bd };
  }

  function shortSched(cat){
    var full = SCHEDULE_MAP[cat]||'';
    // Extract roman numeral from "Schedule X" patterns; default to empty if not found
    var m = /Schedule\s+(I|II|III|IV|V)/i.exec(full);
    return m ? '(' + m[1].toUpperCase() + ')' : '';
  }
  function fullSched(cat){
    return SCHEDULE_MAP[cat] || '';
  }

  function openModalForCategory(cat){
    var terms = Array.isArray(SLANG_MAP[cat]) ? SLANG_MAP[cat] : [];
    var m = buildModal();
    m.title.textContent = 'Drug Slang — ' + cat;
    m.subline.textContent = fullSched(cat);
    if(!terms.length){ m.body.innerHTML = '<div class="muted">No slang terms found for '+escapeHtml(cat)+'.</div>'; }
    else {
      var html = '<div class="item"><div><strong>'+escapeHtml(cat)+'</strong></div>'
               + '<div class="sub">'+escapeHtml(terms.join(', '))+'</div></div>';
      m.body.innerHTML = html;
    }
    document.body.appendChild(m.backdrop);
  }

  // Reverse/fuzzy
  function norm(s){ return String(s||"").toLowerCase().replace(/[^a-z0-9]+/g,'').trim(); }
  function levenshtein(a,b){
    a = norm(a); b = norm(b);
    var m=a.length, n=b.length;
    if(m===0) return n; if(n===0) return m;
    var dp=new Array(n+1);
    for(var j=0;j<=n;j++) dp[j]=j;
    for(var i=1;i<=m;i++){
      var prev=dp[0]; dp[0]=i;
      for(var j=1;j<=n;j++){
        var tmp=dp[j];
        dp[j]=Math.min(dp[j]+1, dp[j-1]+1, prev + (a[i-1]===b[j-1]?0:1));
        prev=tmp;
      }
    }
    return dp[n];
  }
  function similar(a,b){
    a=String(a||""); b=String(b||"");
    var an=norm(a), bn=norm(b);
    if(!an||!bn) return false;
    if(an===bn) return true;
    if(an.length>=3 && bn.indexOf(an)!==-1) return true;
    var d=levenshtein(an,bn);
    if(bn.length<=5) return d<=1;
    if(bn.length<=8) return d<=2;
    return d<=3;
  }
  var termToCats = (function(){
    var idx = Object.create(null);
    var cats = Object.keys(SLANG_MAP);
    for(var i=0;i<cats.length;i++){ var c=cats[i]; var arr=SLANG_MAP[c]||[];
      for(var j=0;j<arr.length;j++){ var k=norm(arr[j]); if(!k) continue; (idx[k]=idx[k]||[]).indexOf(c)===-1 && idx[k].push(c); }
    }
    return idx;
  })();
  function rankCategories(q){
    var s = String(q||"").trim(); if(!s) return [];
    var sn = norm(s);
    var cats = Object.keys(SLANG_MAP);
    var scores = {};
    function setScore(cat, sc){ if(!(cat in scores) || sc<scores[cat]) scores[cat]=sc; }
    for(var i=0;i<cats.length;i++){ var c=cats[i];
      if(similar(s, c)) setScore(c, 0);
      else if(c.toLowerCase().indexOf(s.toLowerCase())!==-1) setScore(c, 1);
    }
    var exactCats = termToCats[sn];
    if(exactCats){ for(var i=0;i<exactCats.length;i++) setScore(exactCats[i], 0); }
    else { for(var i=0;i<cats.length;i++){ var c=cats[i], arr=SLANG_MAP[c]||[]; for(var j=0;j<arr.length;j++){ if(similar(s, arr[j])){ setScore(c, 2); break; } } } }
    return Object.keys(scores).map(function(c){ return {cat:c, score:scores[c]}; })
      .sort(function(a,b){ return a.score===b.score ? a.cat.localeCompare(b.cat) : a.score-b.score; })
      .map(function(x){ return x.cat; });
  }
  function filterCategories(q){ return rankCategories(q).slice(0,8); }

  function buildCard(){
    var card = createEl('div',{class:'card', id:CARD_ID},'');
    card.style.padding = '16px';
    card.style.marginTop = '12px';

    var titleRow = document.createElement('div');
    titleRow.style.cssText='display:flex;justify-content:space-between;align-items:center;margin-bottom:8px';
    var h3 = document.createElement('h3'); h3.textContent='Drug Slang'; h3.style.margin='0';
    var toggle = document.createElement('button'); toggle.className='btn'; toggle.textContent='Show';
    titleRow.appendChild(h3); titleRow.appendChild(toggle);

    var desc = createEl('p',{class:'muted'},'Click Show to search by drug or slang word (e.g., Marijuana or "china").');
    desc.style.margin='0 0 8px 0';

    var searchWrap = document.createElement('div'); searchWrap.style.display='none';
    var input = document.createElement('input'); input.className='input'; input.placeholder='Type a drug or slang word...'; input.id='slang-cat-input';
    var suggest = document.createElement('div'); suggest.id='slang-suggest'; suggest.className='card';
    suggest.style.cssText='display:none;background:#0f1532;color:#e8eefc;border:1px solid #3c4e73;border-radius:12px;padding:6px;margin-top:6px';

    searchWrap.appendChild(input); searchWrap.appendChild(suggest);

    var open = localStorage.getItem(TOGGLE_KEY) === '1';
    function apply(){ if(open){ searchWrap.style.display=''; toggle.textContent='Hide'; } else { searchWrap.style.display='none'; toggle.textContent='Show'; } }
    apply();
    toggle.addEventListener('click', function(){ open = !open; localStorage.setItem(TOGGLE_KEY, open ? '1' : '0'); apply(); });

    function renderSuggest(list){
      if(!list.length){ suggest.style.display='none'; suggest.innerHTML=''; return; }
      suggest.style.display = 'block';
      var html = '';
      for(var i=0;i<list.length;i++){
        var cat=list[i];
        var label = cat + ' ' + shortSched(cat); // compact (I)/(II)/...
        html += '<div class="item" data-cat="'+escapeHtml(cat)+'" style="padding:8px;cursor:pointer;border-bottom:1px solid #243154">'+label+'</div>';
      }
      suggest.innerHTML = html;
      Array.prototype.forEach.call(suggest.querySelectorAll('.item'), function(it){
        it.addEventListener('click', function(){ openModalForCategory(this.getAttribute('data-cat')); });
      });
    }
    input.addEventListener('input', function(){ renderSuggest(filterCategories(this.value)); });
    input.addEventListener('keydown', function(e){
      if(e.key==='Enter'){
        var val=(this.value||'').trim();
        if(SLANG_MAP[val]) openModalForCategory(val);
        else { var list=rankCategories(val); if(list.length===1) openModalForCategory(list[0]); else renderSuggest(list.slice(0,8)); }
      }
    });

    card.appendChild(titleRow); card.appendChild(desc); card.appendChild(searchWrap);
    return card;
  }

  function escapeHtml(s){ return String(s).replace(/&/g,"&amp;").replace(/</g,"&lt;").replace(/>/g,"&gt;").replace(/"/g,"&quot;").replace(/'/g,"&#039;"); }

  function ensureCardExists(){
    if(document.getElementById(CARD_ID)) return;
    var cards = document.querySelectorAll('.card');
    var pillCard=null; for(var i=0;i<cards.length;i++){ if(looksLikePillCard(cards[i])){ pillCard=cards[i]; break; } }
    if(!pillCard) return;
    var card = buildCard();
    pillCard.parentNode.insertBefore(card, pillCard.nextSibling);
  }

  var obs = new MutationObserver(function(){ ensureCardExists(); });
  obs.observe(document.documentElement, {childList:true, subtree:true});
  // ==== APP ENTRYPOINT (DOMContentLoaded) ====
// Purpose: bootstraps the UI once the DOM is ready (tabs, flowchart, modal wiring).
document.addEventListener('DOMContentLoaded', ensureCardExists);
  window.addEventListener('load', ensureCardExists);
  var n=0; var timer=setInterval(function(){ if(++n>50) return clearInterval(timer); ensureCardExists(); }, 200);
})();

</body>
</html>
